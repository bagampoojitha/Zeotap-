-- SQL table to store rules
CREATE TABLE rules (
    id INT PRIMARY KEY,
    rule_name VARCHAR(50),
    rule_expression TEXT
);

-- Example rules
INSERT INTO rules (rule_name, rule_expression) VALUES 
('age_check', 'age >= 18'),
('income_check', 'income > 50000'),
('spend_check', 'spend < 20000'),
('department_check', "department == 'Finance'");
